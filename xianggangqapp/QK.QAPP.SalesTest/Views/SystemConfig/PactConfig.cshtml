@Scripts.Render("~/bundles/bootbox")
<script src="~/Content/assets/js/jquery-ui-1.10.3.full.min.js"></script>
<link href="~/Content/assets/css/jquery-ui-1.10.3.full.min.css" rel="stylesheet" />
<div class="row">
    <div class="col-xs-12">
        <div class="widget-box">
            <div class="widget-header widget-header-small">
                <h5 class="lighter">选择协议</h5>
            </div>
            <div class="widget-body">
                <div class="widget-main">
                    <form class="form-horizontal" id="pact_form">
                        <div class="row">
                            <div class="form-group">
                                <label class="control-label col-xs-12 col-sm-1">协议</label>
                                <div class="col-xs-12 col-sm-6">
                                    <select class="form-control" multiple="multiple" id="pactCode" name="pactCode">
                                        <option>委托划扣授权书</option>
                                        <option>还款事项提醒函</option>
                                        <option>借款协议</option>
                                        <option>借款咨询与服务协议</option>
                                    </select>
                                </div>

                                <div class="col-xs-12 col-sm-1">
                                    <button class="btn btn-purple" type="button" id="deletePact">删除协议</button>
                                </div>
                            </div>
                        </div>
                    </form>

                </div>
            </div>
        </div>
    </div>
</div>


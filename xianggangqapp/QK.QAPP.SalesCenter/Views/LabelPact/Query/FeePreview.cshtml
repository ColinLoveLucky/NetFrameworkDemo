            case 1: {
                //currentGrid = grid_selector_plane;
                currentHighSearch = "reissue_highSearch_plane";
            }
                break;
            case 2: {
                //currentGrid = grid_selector_advance;
                currentHighSearch = "reissue_highSearch_advance";
            }
                break;
            case 3: {
                //currentGrid = grid_selector_delay;
                currentHighSearch = "reissue_highSearch_delay";
            }
                break;
            default: break;
        }
        jQuery(".tab-pane").hide();
        jQuery(jQuery(obj).attr('href') + "-1").show();
        jQuery(obj).parent().parent().find("li.active").removeClass('active');
        jQuery(obj).parent().addClass('active');
    }

    /*开启/关闭高级搜索*/
    function showHighSearch() {
        jQuery("#" + currentHighSearch).toggle(300, function () { jQuery(window).resize() });
    }

</script>